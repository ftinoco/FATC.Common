<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Fernando Tinoco</Authors>
    <Product>Fernando Tinoco's common library</Product>
    <Description>Helper library with logger manager, result object, any helper extensions</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>Log4Net Nlog Ftinococ Helpers Common</PackageTags>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/ftinoco/FATC.Common</RepositoryUrl>
    <Version>1.0.4.3</Version>
    <RepositoryType>github</RepositoryType>
    <PackageProjectUrl>https://github.com/ftinoco/FATC.Common</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Extensions\ObjectExtensions.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="log4net" Version="2.0.8" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="NLog" Version="4.6.8" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.6.0" />
  </ItemGroup>

</Project>
